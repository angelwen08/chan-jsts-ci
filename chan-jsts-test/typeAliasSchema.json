{
    "id": "https://example.com/arrays.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A representation of a type alias infomation",
    "type": "object",
    "properties":{
        "targetType":{
            "type":"string"
        },
        "elements":{
            "type":"array",
            "items": {"$ref": "#/definitions/typeContent" }

        }
    },
    "definitions":{
        "typeContent":{
            "type":"object",
            "properties": {
                "name":{
                    "type":"string"
                },
                "syntax":{
                    "type":"object",
                    "properties": {
                        "content":{
                            "type":"string"
                        },
                        "return":{
                            "type":"array",
                            "items": {
                                "type":"string"
                            }
                        },
                        "parameters":{
                            "type":"array",
                            "items":{
                                "type":"object",
                                "properties":{
                                    "id":{
                                        "type":"string"
                                    },
                                    "type":{
                                        "type":"string"
                                    }
                                }
                            }   
                        }
                    }    
                }
            }
        }
    }




}